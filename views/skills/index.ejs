<!DOCTYPE html>
<html lang="en">

<head>
  <%- include('../partials/head'); %>
</head>

<body>

  <header>
    <%- include('../partials/header'); %>
  </header>


  <h1>All Skills</h1>


  <ol>
    <% skills.forEach(function(s) { %>
      <li>
       <a href="/skills/<%= s.id %>"><%= s.skill %></a> 
          -
          <%= s.proficiency %>
      </li>
      <% }); %>
    </ol>

  <footer>
    <%- include('../partials/footer'); %>
  </footer>

</body>

</html>